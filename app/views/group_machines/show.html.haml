.row.justify-content-center
  .col-sm-8.col-lg-6
    .card
      .card-header
        Group Machine details
      .card-body
        %dl.row.mb-0
          %dt.col-md-3 Groupid:
          %dd.col-md-9= @group_machine.groupId
          %dt.col-md-3 Processid:
          %dd.col-md-9= @group_machine.processId
      .card-footer.d-flex
        = link_to 'Edit', edit_group_machine_path(@group_machine), class: 'btn btn-link'
        = link_to 'Back', group_machines_path, class: 'btn btn-link ms-auto'